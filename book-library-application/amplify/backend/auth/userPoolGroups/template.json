{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Parameters": {
        "authbookLibraryAppAuthUserPoolId": {
            "Type": "String",
            "Default": "authbookLibraryAppAuthUserPoolId"
        },
        
        "authbookLibraryAppAuthAppClientID": {
            "Type": "String",
            "Default": "authbookLibraryAppAuthAppClientID"
        },
        "authbookLibraryAppAuthAppClientIDWeb": {
            "Type": "String",
            "Default": "authbookLibraryAppAuthAppClientIDWeb"
        },
        "AuthRoleArn": {
            "Type": "String"
        },
        "UnauthRoleArn": {
            "Type": "String"
        },
        "env": {
            "Type": "String"
        }
    },
    "Conditions": {
        "ShouldNotCreateEnvResources": {
            "Fn::Equals": [
                {
                    "Ref": "env"
                },
                "NONE"
            ]
        }
    },
    "Resources": {
    
        "adminsGroup": {
          "Type" : "AWS::Cognito::UserPoolGroup",
          "Properties" : {
              "GroupName" : "admins",
              "Precedence" : 1,
              
              "UserPoolId" : {
                "Ref": "authbookLibraryAppAuthUserPoolId"
              } 
            }
        },
        
    
        "applicationUsersGroup": {
          "Type" : "AWS::Cognito::UserPoolGroup",
          "Properties" : {
              "GroupName" : "applicationUsers",
              "Precedence" : 2,
              
              "UserPoolId" : {
                "Ref": "authbookLibraryAppAuthUserPoolId"
              } 
            }
        },
        
    
    
    },
    
}
